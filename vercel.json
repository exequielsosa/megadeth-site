{
  "redirects": [
    {
      "source": "/songs",
      "has": [
        {
          "type": "host",
          "value": "www.megadeth.com.ar"
        }
      ],
      "destination": "https://megadeth.com.ar/songs",
      "permanent": true
    },
    {
      "source": "/songs/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.megadeth.com.ar"
        }
      ],
      "destination": "https://megadeth.com.ar/songs/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.megadeth.com.ar"
        }
      ],
      "destination": "https://megadeth.com.ar/$1",
      "permanent": true
    }
  ]
}
